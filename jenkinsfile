pipeline {
    agent any

    stages {
        stage('Get code') {
            steps {
                git branch: 'main', url: 'https://github.com/Saketh227/one.git'
            }
        }
        
        stage("build container") {
            agent {
                dockerfile {
                    filename 'dockerfile'
                }
            }
            steps{
                sh 'touch file1'
            }
    }
}
